    "@types/jest": "^27.0.2",
    "consign": "^0.1.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^27.2.4",
    "knex": "^0.95.11",
    "pg": "^8.7.1",
